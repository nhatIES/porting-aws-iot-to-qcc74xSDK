+        else:
+            raise Exception('Unknown app type: %r' % self)
+
+    def FlashBundleName(self):
+        if self == Qcc74xApp.LIGHT:
+            return 'lighting_app.flashbundle.txt'
+        else:
+            raise Exception('Unknown app type: %r' % self)
