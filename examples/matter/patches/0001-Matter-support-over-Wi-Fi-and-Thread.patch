+#else
+#ifdef CFG_USE_PSRAM
+    currentHeapUsed = (get_heap_size() + get_heap3_size() - xPortGetFreeHeapSize() - xPortGetFreeHeapSizePsram());
+#else
+    currentHeapUsed = (get_heap_size() - xPortGetFreeHeapSize());
