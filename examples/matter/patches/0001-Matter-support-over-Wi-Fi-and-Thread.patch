+}
+#else
+void AppTask::AppShellTask(void * args)
+{
+    Engine::Root().RunMainLoop();
+}
+
+CHIP_ERROR AppTask::StartAppShellTask()
+{
+    static TaskHandle_t shellTask;
+
+    Engine::Root().Init();
