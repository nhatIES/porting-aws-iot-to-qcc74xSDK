+
+class Qcc74xBoard(Enum):
+    qcc743DK = auto()
+
+    def GnArgName(self):
+        if self == Qcc74xBoard.qcc743DK:
+            return 'qcc743DK'
+        else:
+            raise Exception('Unknown board #: %r' % self)
+
+
+class Qcc74xMfd(Enum):
