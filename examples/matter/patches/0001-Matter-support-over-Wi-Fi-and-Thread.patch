+    currentHeapFree = kfree_size();
+#else
+#ifdef CFG_USE_PSRAM
+    currentHeapFree = xPortGetFreeHeapSize() + xPortGetFreeHeapSizePsram();
+#else
+    currentHeapFree = xPortGetFreeHeapSize();
+#endif
